{
  "expo": {
    "name": "SOS Afia",
    "slug": "sos-afia-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fasofood.davme",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSCameraUsageDescription": "Fasofood démande accès à la camera pour scanner le QRCode au processus de paiement de transport",
        "NSPhotoLibraryUsageDescription": "Fasofood démande un accès à votre librairie pour vous pemettre de séléctionner une image de profile",
        "NSUserActivityTypes": [
          "INSendMessageIntent"
        ]
      }
    },
    "android": {
      "package": "com.fasofood.davme",
      "versionCode": 17,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "assets/icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ]
  }
}
